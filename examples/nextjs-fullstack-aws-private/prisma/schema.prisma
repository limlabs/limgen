generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  description String?
  content   String?
  coverImageUrl String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt @default(now())
}
