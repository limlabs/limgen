{
  "name": "limgen",
  "version": "0.0.21",
  "description": "Infrastructure as Code generator",
  "bin": "./dist/bin/index.js",
  "engines": {
    "node": ">=22.11.0"
  },
  "scripts": {
    "prepublish": "pnpm build",
    "dev": "tsx src/bin/index.ts",
    "test": "rm -rf test/output && tsx --test --experimental-test-module-mocks  --experimental-test-coverage --test-coverage-exclude='test/**/*.ts' test/*.ts",
    "build": "scripts/build.sh",
    "update-examples": "scripts/update-examples.sh"
  },
  "keywords": [
    "iac",
    "infrastructure",
    "pulumi",
    "aws",
    "typescript"
  ],
  "author": "Liminal Labs",
  "license": "MIT",
  "devDependencies": {
    "@pulumi/aws": "^6.59.1",
    "@pulumi/awsx": "^2.18.1",
    "@pulumi/pulumi": "^3.138.0",
    "@pulumi/random": "^4.16.7",
    "@types/node": "^22.9.0",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "tsc-alias": "1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/ejs": "^3.1.5",
    "@types/js-yaml": "^4.0.9",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "js-yaml": "^4.1.0",
    "ora": "8.1.1",
    "prompts": "^2.4.2",
    "zod": "^3.23.8"
  }
}
